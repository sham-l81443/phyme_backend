{
  "name": "backendqz",
  "version": "1.0.0",
  "description": "Quiz app ",
  "main": "server.ts",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "test:register": "artillery run tests/perfomance/register-test.yaml",
    "seed": "ts-node prisma/seed-comprehensive.ts",
    "serve": "pm2 start dist/server.js --name phyme_BE",
    "start": "node ./dist/server.js",
    "setup-pgbouncer": "bash scripts/setup-pgbouncer.sh",
    "setup-pgbouncer-native": "bash scripts/install-pgbouncer-native.sh",
    "pgbouncer:start": "docker-compose -f docker-compose.pgbouncer.yml up -d",
    "pgbouncer:stop": "docker-compose -f docker-compose.pgbouncer.yml down",
    "pgbouncer:logs": "docker-compose -f docker-compose.pgbouncer.yml logs -f",
    "pgbouncer:start-native": "launchctl start com.pgbouncer",
    "pgbouncer:stop-native": "launchctl stop com.pgbouncer",
    "pgbouncer:restart-native": "launchctl unload ~/Library/LaunchAgents/com.pgbouncer.plist && launchctl load ~/Library/LaunchAgents/com.pgbouncer.plist",
    "pgbouncer:logs-native": "tail -f /usr/local/var/log/pgbouncer/pgbouncer.log",
    "pgbouncer:monitor": "node scripts/monitor-pgbouncer.js",
    "pgbouncer:test": "node scripts/test-pgbouncer.js",
    "health": "curl http://localhost:3001/api/health",
    "pgbouncer:stats": "curl http://localhost:3001/api/health/pgbouncer"
  },
  "prisma": {
    "schema": "./prisma",
    "seed": "ts-node prisma/seed-comprehensive.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "@supabase/supabase-js": "^2.47.0",
    "@types/passport-google-oauth20": "^2.0.16",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "otp-generator": "^4.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pm2": "^6.0.8",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@types/nodemailer": "^6.4.17",
    "@types/otp-generator": "^4.0.2",
    "artillery": "^2.0.21",
    "prisma": "^6.9.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "keywords": []
}
