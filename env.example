# Database Configuration
DATABASE_URL="postgresql://postgres:your_password@localhost:5432/phyme_db?schema=public"
PGBOUNCER_URL="postgresql://postgres:your_password@localhost:6432/phyme_db?schema=public&pgbouncer=true"

# Application Configuration
NODE_ENV="development"
PORT=3001
JWT_SECRET="your_jwt_secret_here"

# CORS Configuration
CORS_ENABLED=true

# Email Configuration (if using)
SMTP_HOST="smtp.gmail.com"
SMTP_PORT=587
SMTP_USER="your_email@gmail.com"
SMTP_PASS="your_app_password"

# Google OAuth (if using)
GOOGLE_CLIENT_ID="your_google_client_id"
GOOGLE_CLIENT_SECRET="your_google_client_secret"

# Supabase (if using)
SUPABASE_URL="your_supabase_url"
SUPABASE_ANON_KEY="your_supabase_anon_key"

# PgBouncer Configuration
PGBOUNCER_HOST="localhost"
PGBOUNCER_PORT=6432
PGBOUNCER_DATABASE="phyme_db"
PGBOUNCER_USER="postgres"
PGBOUNCER_PASSWORD="your_password"

# Connection Pool Settings
DB_POOL_MIN=5
DB_POOL_MAX=20
DB_POOL_IDLE_TIMEOUT=30000
DB_POOL_ACQUIRE_TIMEOUT=60000
